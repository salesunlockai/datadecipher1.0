<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no' name='viewport' />
    <title>@ViewData["Title"] - DataDecipher</title>
    <!--     Fonts and icons     -->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700,200" rel="stylesheet" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/assets/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/assets/css/now-ui-kit.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/assets/css/simple-sidebar.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/assets/css/jquery.dataTables.min.css" />
</head>
<body class="profile-page sidebar-collapse">
    @await Html.PartialAsync("_HeaderNavBar")
    <div id="wrapper" class="Wrapper">
        @await Html.PartialAsync("~/Views/Shared/_SideBar.cshtml")
        <div class="section">
                @RenderBody()
        </div>
    </div>
        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
        <script src="~/lib/bootstrap/assets/js/jquery.dataTables.min.js"></script>
        <script src="~/lib/bootstrap/assets/js/now-ui-kit.js"></script>
        <script>$("#menu-toggle").click(function(e) {
                e.preventDefault();
                $("#wrapper").toggleClass("toggled");
});</script>
        <script>$(document).ready(function() {
                $('#myTable').dataTable();
});</script>

        @RenderSection("Scripts", required: false)

</body>
</html>
